{
  "name": "molecule-3d-for-react",
  "version": "0.4.2",
  "description": "3D molecule visualization with React and 3Dmol.js",
  "engines": {
    "node": "6.9.1"
  },
  "main": "dist/bundle.js",
  "files": [
