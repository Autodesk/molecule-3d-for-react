    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
