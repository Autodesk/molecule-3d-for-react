    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "nightwatch": "^0.9.8",
    "node-sass": "^3.6.0",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^3.2.0",
    "selenium-download": "^2.0.10",
    "sinon": "^2.0.0-pre.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "enzyme": "^2.7.1",
    "json-loader": "^0.5.4"
  },
  "scripts": {
    "test": "java -version && karma start --single-run",
    "tdd": "karma start",
